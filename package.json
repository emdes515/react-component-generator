{
	"name": "react-component-generator-by-Mateusz-Jankowski",
	"displayName": "React Component Generator by Mateusz Jankowski",
	"description": "A Visual Studio Code extension that simplifies React component creation, supporting functional components, container-presentation pairs, TypeScript, and PropTypes.",
	"author": "Mateusz Jankowski",
	"publisher": "MateuszJankowskii",
	"license": "MIT",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.92.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/emdes515/react-component-generator"
	},
	"icon": "images/icon.png",
	"categories": [
		"Extension Packs"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "react-component-generator.createComponent",
				"title": "Create React Component"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/vscode": "^1.92.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"eslint": "^8.57.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.92.1",
		"webpack-cli": "^5.1.4"
	}
}
